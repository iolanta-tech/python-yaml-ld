SELECT ?test ?result ?input ?req WHERE {
    ?test
        a $test_class ;
        mf:result ?result ;
        mf:action ?input .

    OPTIONAL {
        ?test jld:req ?req .
    }

    # We mimic `pyld` tests here. Skipping 1.0 tests.
    FILTER NOT EXISTS {
        ?test jld:option / jld:specVersion "json-ld-1.0"^^xsd:string .
    }
}
